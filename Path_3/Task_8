-- 8. List all unrequited followings (i.e. where A follows B but B does not follow A).
WITH VT_NUMBERS AS (
	SELECT USER_ID, FOLLOWS, 
	(USER_ID::VARCHAR||''||FOLLOWS::VARCHAR) AS USER_NB, (FOLLOWS::VARCHAR||''||USER_ID::VARCHAR) AS FOLLOWS_NB, 
	DB_DATE
	FROM FOLLOWS
	)
SELECT VT.USER_ID, VT.FOLLOWS
FROM VT_NUMBERS VT
LEFT JOIN
VT_NUMBERS VT_1
ON VT.USER_NB=VT_1.FOLLOWS_NB
WHERE VT_1.USER_ID IS NULL
